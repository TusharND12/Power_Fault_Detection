# Watson Assistant Configuration Template
# Copy this file to .env.local and update with your actual values

# Watson Assistant API Key
ASSISTANT_APIKEY=your_watson_api_key_here

# Watson Assistant IAM API Key (same as above for IAM authentication)
ASSISTANT_IAM_APIKEY=your_watson_iam_api_key_here

# Watson Assistant Service URL
ASSISTANT_URL=https://api.us-south.assistant.watson.cloud.ibm.com/instances/your_instance_id

# Watson Assistant Authentication Type
ASSISTANT_AUTH_TYPE=iam

# Watson Assistant Version
ASSISTANT_VERSION=2023-11-22

# Watson Assistant ID
ASSISTANT_ID=your_assistant_id_here

# Environment
NODE_ENV=development
